{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "format": {},
    "test": {
      "dependsOn": ["build", "^build"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  },
  "globalDependencies": []
}
